
    <div class="container">
        <h2>Your Cart</h2>
        <% cart.forEach((item) => { %>
            <li>
                <h3><%= item %></h3>
            </li>
        <% }); %>
        <% if (cart.length > 0) { %>
            <ul>
                <% cart.forEach((item) => { %>
                    <li>
                        <h3><%= item %></h3>
                    </li>
                <% }); %>
            </ul>
        <% } else { %>
            <p>Your cart is empty.</p>
        <% } %>
    </div>

  